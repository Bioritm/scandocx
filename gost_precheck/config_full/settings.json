{
  "version": "1.4",
  "numbering_scope": "global",
  "regex_workers": 0,

  "normalize": {
    "dashes": true,
    "quotes": true,
    "nbsp": true,
    "strip_zw": true
  },
  "post_normalize": {
    "dashes": false,
    "quotes": false
  },

  "loader": {
    "include_styles": true,
    "include_tabs": true
  },

  "spell": {
    "enabled": true,
    "parallel_workers": 0,
    "min_len": 3,
    "skip_upper": true,
    "suggestions": true,
    "max_suggestions_per_word": 3,
    "ru": true,
    "en": false,
    "dict_dir": "auto",
    "pwl_path": ""
  },

  "warnings": {
    "WS_TRAILING": false
  }
}
